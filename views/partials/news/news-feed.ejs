<div class="vstack gap-3 bg-white py-2">
   <% for (let i = start; i < end; i++) {%>
      <% if (i >= 15 && (i-2) % 6 == 0) { %>
         <div class="bg-light p-2">
            <%- include('./preview-main-news', { newsData: main[1] }); -%>
         </div>
      <% } else {%>
         <div class="fs-6 lh-sm px-2"> <span class="px-3 fw-bold" style="font-size: 1rem; color: #eb1a1a;">15:12</span>Lorem ipsum dolor sit amet consectetur adipisicing elit. Recusandae, consequuntur?</div>
      <% } %>

      <% if (end != i + 1) { %>
      <hr class="my-1">
      <% } %>

   <% }%>
</div>